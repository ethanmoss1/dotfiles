# Host options
Host server
	 User		root
     Hostname   192.168.0.11

# Specifically for notmuch email when accessesing on a remote server
Host notmuch
	 User			root
     Hostname   	192.168.0.64
     ControlMaster 	auto
     ControlPath 	~/.ssh/master-%h@%p:%r
     ControlPersist 15m

Host nixos
	 Hostname	192.168.0.251

